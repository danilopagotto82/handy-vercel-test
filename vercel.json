{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/meta$",
      "dest": "/api/meta.js"
    },
    {
      "src": "^/api/meta$",
      "dest": "/api/meta.js"
    },
    {
      "src": "^/auth/device$",
      "dest": "/api/auth/device.js"
    },
    {
      "src": "^/auth/callback$",
      "dest": "/api/auth/callback.js"
    },
    {
      "src": "^/api/(.*)$",
      "dest": "/api/index.js"
    }
  ]
}
